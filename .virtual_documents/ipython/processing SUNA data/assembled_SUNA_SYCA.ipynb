    SYCAday$PAR_roll<-as.numeric(c("NA",rollmean(SYCAday$PARMean.mean,k=3,align=c("center"),na.fill=TRUE),"NA"))
    SYCAday$s275295_roll<-as.numeric(c("NA",rollmean(SYCAday$s275295,k=3,align=c("center"),na.fill=TRUE),"NA"))



    SYCA_night<-subset(SYCAday,PARMean.mean<200)
    night_s275295<-mean(SYCA_night$s275295,na.rm=TRUE)
    
    #SYCAday$baseline<-predict(lm(s275295~dtpr,SYCA_night),SYCAday)
    SYCAday$baseline_roll<-predict(lm(s275295_roll~dtpr,SYCA_night),SYCAday)    
