    day_PAR_roll_base_sum<-sum(SYCA_day$PAR_roll,na.rm=TRUE)    

    int<-lm(s275295_bs~PAR_roll,SYCA_day)$coef[1]
    slope<-lm(s275295_bs~PAR_roll,SYCA_day)$coef[2]    
    
